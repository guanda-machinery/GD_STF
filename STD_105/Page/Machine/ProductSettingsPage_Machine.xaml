                                                         Style="{StaticResource rad_Gray}"
                                                         IsChecked="{Binding Path=rbtn_DrillingFace, Mode=TwoWay, Converter={local:DrillingFaceConverter}, ConverterParameter=0}"
                                                         Content="頂面"/>
                                            <RadioButton  Grid.Column="0" GroupName="DrillingPosition" Foreground="Black" HorizontalAlignment="Center"
                                                          Style="{StaticResource rad_Gray}"
                                                          IsChecked="{Binding Path=rbtn_DrillingFace, Mode=TwoWay, Converter={local:DrillingFaceConverter}, ConverterParameter=1}"
                                                          Content="前面"/>

                                            <RadioButton Grid.Column="2" GroupName="DrillingPosition" Foreground="Black" HorizontalAlignment="Center"
                                                          Style="{StaticResource rad_Gray}" 
                                                          IsChecked="{Binding Path=rbtn_DrillingFace, Mode=TwoWay, Converter={local:DrillingFaceConverter}, ConverterParameter=2}"
                                                           Content="後面"/>


                                        </Grid>
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox Header="孔群設定" Grid.Column="0" Grid.Row="2">
