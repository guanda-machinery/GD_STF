                            </Style>
                        </StackPanel.Resources>
                        <local:AssembledControl3 LabelText="{StaticResource LANGUAGE_TestPoint}" />
                        <local:AssembledControl3 LabelText="{StaticResource LANGUAGE_Tap}" />
