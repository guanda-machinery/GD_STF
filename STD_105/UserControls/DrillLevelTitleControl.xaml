<UserControl x:Class="STD_105.DrillLevelTitleControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:STD_105"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <StackPanel.Resources>
            <Style TargetType="TextBlock" BasedOn="{ StaticResource titleRow }">
                <Setter Property="Width" Value="120"/>
                <Setter Property="Height" Value="25"/>
                <Setter Property="Padding" Value="5"/>
            </Style>
        </StackPanel.Resources>
        <StackPanel Orientation="Horizontal">
                <TextBlock Text="Level1"/>
                <TextBlock Text="Level2"/>
                <TextBlock Text="Level3"/>
                <TextBlock Text="Level4"/>
            </StackPanel >
            <StackPanel Orientation="Horizontal">
                <TextBlock Text="HHS"/>
                <TextBlock Text="HSS油孔"/>
                <TextBlock Text="鎢化鋼刀片"/>
                <TextBlock Text="鎢化鋼刀片+"/>
            </StackPanel>
        </StackPanel>
</UserControl>
