<WPFBase:BasePage x:Class="STD_105.Office.ParametersSettings_Office"
                  xmlns:WPFBase="clr-namespace:WPFWindowsBase;assembly=WPFWindowsBase"
                  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                  xmlns:STD105VM="clr-namespace:WPFSTD105.ViewModel;assembly=WPFSTD105"
                  xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
                  xmlns:wpf="http://materialdesigninxaml.net/winfx/xaml/themes"
                  xmlns:SofSetting="clr-namespace:WPFSTD105.Properties;assembly=WPFSTD105"
                  xmlns:std="clr-namespace:STD_105"
                  xmlns:WPFSTD="clr-namespace:WPFSTD105;assembly=WPFSTD105"
                  xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
                  xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
                  xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
                  xmlns:local="clr-namespace:STD_105.Office"
                  xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
                  xmlns:uc_language="clr-namespace:LanguageSettingDialog.View"
                  xmlns:uc_sticker="clr-namespace:StickerDialog.View"
                   mc:Ignorable="d" 
                  xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm" xmlns:STD105ValidRule="clr-namespace:STD_105.ValidationRules" xmlns:ST="clr-namespace:SectionData;assembly=WPFSTD105" xmlns:LanguageVM="clr-namespace:LanguageSettingDialog.ViewModel" xmlns:StickerDialogVM="clr-namespace:StickerDialog.ViewModel"
                  Width="{Binding OfficeViewModel.PageHostWidth, Source={x:Static WPFSTD:ViewLocator.Instance}}"
                   Height="{Binding OfficeViewModel.PageHostHeight, Source={x:Static WPFSTD:ViewLocator.Instance}}"
                  
                  Title="參數設定 - 選單功能">
    <WPFBase:BasePage.DataContext>
        <STD105VM:OfficeVM_ParaSettingFuncListViewModel/>
    </WPFBase:BasePage.DataContext>
    <Grid>
        <TabControl Style="{ StaticResource tabcontrol_Vertical }">
            <TabControl.Resources>
                <Style TargetType="Label"
                       BasedOn="{ StaticResource lab_Title }">
                    <Setter Property="Foreground"
                            Value="{ StaticResource solidclr_ButtonRed }" />
                </Style>
                <Style TargetType="TabItem"
                       BasedOn="{ StaticResource tabitem_Vertical }"></Style>
            </TabControl.Resources>
            <!--新建專案-->
            <TabItem Header="斷面規格目錄">
                <Border BorderThickness="1" BorderBrush="Gray">
                    <std:SectionSpecificationMenu_UserControl  Style="{StaticResource SectionSpecificationMenuWhiteStyle}"/>
                </Border>



            </TabItem>
            <!--專案瀏覽-->
            <TabItem Header="切割線設定">
                <Grid dx:ThemeManager.ThemeName="Win10Light">
                    <Grid.DataContext>
                        <STD105VM:SettingParVM/>
                    </Grid.DataContext>
                    <Grid.Resources>
                        <SolidColorBrush x:Key="PrimaryHueMidBrush" Color="{StaticResource WordBlue}"/>
                        <Style TargetType="Button"  BasedOn="{StaticResource SiegCustomButtonWhite}"   >
                            <Setter Property="BorderThickness" Value="1"/>
                            <Setter Property="FontSize" Value="14"/>
                        </Style>

                        <Style TargetType="Label">
                            <Setter Property="FontSize" Value="12"/>
                        </Style>


                        <Style TargetType="TextBox" BasedOn="{StaticResource MaterialDesignTextBoxBase}">
                            <Setter Property="Foreground" Value="{ StaticResource solidclr_DimGray }" />
                            <Setter Property="FontSize" Value="14" />
                            <Setter Property="Padding" Value="1" />
                            <Setter Property="FontFamily" Value="Microsoft JhengHei"/>
                            <Setter Property="Background" Value="White"/>
                            <Setter Property="BorderThickness" Value="1"/>
                            <Setter Property="BorderBrush" Value="Gray"/>
                        </Style>

                        <Style TargetType="CheckBox" BasedOn="{StaticResource BaseStyle}">
                            <Setter Property="VerticalAlignment" Value="Center"/>
                        </Style>
                        <Style TargetType="ComboBox"
                   BasedOn="{ StaticResource BaseStyle }">
                            <Setter Property="Height" Value="26"/>
                            <Setter Property="FontFamily" Value="10"/>
                        </Style>


                    </Grid.Resources>
                    <!--切割線設定-->
                    <Border BorderBrush="Gray" BorderThickness="1">
                        <GroupBox Header="切割線設定" FontSize="16" Style="{StaticResource ParameterGroupBox}">
                            <Grid MinWidth="500">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="auto"/>
                                    <RowDefinition Height="auto"/>
                                </Grid.RowDefinitions>
                                <!--關鍵字過濾-->

                                <StackPanel Grid.Row="0" Orientation="Horizontal" HorizontalAlignment="Center">
                                    <Label VerticalAlignment="Center" HorizontalContentAlignment="Center" Content=" 腹板切割記號位置" FontSize="16" Margin="5" />
                                    <Grid>
                                        <Image Source="{ StaticResource WEB_CUT_MARK }" Height="170" Width="170"/>
                                        <Canvas>
                                            <Grid Canvas.Left="29" Canvas.Top="112" Height="25" Width="156">
                                                <TextBlock Text="A" FontFamily="Times New Roman" FontWeight="UltraBold"/>
                                            </Grid>
                                        </Canvas>
                                        <Canvas>
                                            <Grid Canvas.Left="106" Canvas.Top="90" Height="25" Width="156">
                                                <TextBlock Text="B" FontFamily="Times New Roman" FontWeight="UltraBold"/>
                                            </Grid>
                                        </Canvas>
                                    </Grid>
                                </StackPanel>
                                <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Center" >
                                    <Label VerticalAlignment="Center" HorizontalContentAlignment="Center" Content=" 翼板切割記號位置" FontSize="16"  Margin="5"/>
                                    <Grid>
                                        <Image Source="{ StaticResource WING_CUT_MARK }" Height="170" Width="170"/>
                                        <Canvas>
                                            <Grid Canvas.Left="29" Canvas.Top="115" Height="25" Width="156">
                                                <TextBlock Text="C" FontFamily="Times New Roman" FontWeight="UltraBold"/>
                                            </Grid>
                                        </Canvas>
                                        <Canvas>
                                            <Grid Canvas.Left="106" Canvas.Top="90" Height="25" Width="156">
                                                <TextBlock Text="D" FontFamily="Times New Roman" FontWeight="UltraBold"/>
                                            </Grid>
                                        </Canvas>
                                    </Grid>
                                </StackPanel>

                                <!--切割線設定-->
                                <GroupBox Grid.Row="2" Header="切割線設定" Background="White" Style="{StaticResource ParameterBaseStyleGroupBox}">
                                    <Grid Margin="10,0" Height="275">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="auto" />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition Width="auto"/>
                                            <ColumnDefinition MaxWidth="200" />
                                            <ColumnDefinition Width="auto"/>
                                        </Grid.ColumnDefinitions>

                                        <CheckBox Grid.Row="0" Grid.Column="0" Visibility="Hidden" />
                                        <Label Content="選擇記號比例"  Grid.Row="0" Grid.Column="1" VerticalAlignment="Center" />
                                        <ComboBox Grid.Row="0" Grid.Column="2" x:Name="cbx_HowManyParts" SelectedValue="{Binding HowManyParts_Value}" ItemsSource="{Binding cbb_HowManyParts}" >
                                            <i:Interaction.Triggers>
                                                <i:EventTrigger EventName="SelectionChanged" SourceName="cbx_HowManyParts">
                                                    <i:InvokeCommandAction Command="{ Binding ShowHowManyPartsRelatedComboboxCommand }"
                                                               CommandParameter="{ Binding ElementName=cbx_HowManyParts, Path=SelectedValue }" />
                                                </i:EventTrigger>
                                            </i:Interaction.Triggers>
                                        </ComboBox>
                                        <Label Content="等分" Grid.Row="0" Grid.Column="3" VerticalAlignment="Center" Margin="3,0"/>

                                        <CheckBox Grid.Row="1" Grid.Column="0" IsChecked="{Binding chb_PointA_SplitLineSetting}"/>
                                        <Label Content="A值(打點位置)"  Grid.Row="1" Grid.Column="1" VerticalAlignment="Center"/>
                                        <ComboBox Grid.Row="1" Grid.Column="2" SelectedValue="{Binding PointA_Value}" ItemsSource="{Binding cbb_A_ItemSource}"/>

                                        <CheckBox Grid.Row="2" Grid.Column="0" IsChecked="{Binding chb_PointB_SplitLineSetting}" />
                                        <Label Content="B值(打點位置)"  Grid.Row="2" Grid.Column="1" VerticalAlignment="Center" />
                                        <ComboBox Grid.Row="2" Grid.Column="2" SelectedValue="{Binding PointB_Value}" ItemsSource="{Binding cbb_B_ItemSource}" />

                                        <CheckBox Grid.Row="3" Grid.Column="0" IsChecked="{Binding chb_PointC_SplitLineSetting}" />
                                        <Label Content="C值(打點位置)"  Grid.Row="3" Grid.Column="1" VerticalAlignment="Center" />
                                        <ComboBox Grid.Row="3" Grid.Column="2" SelectedValue="{Binding PointC_Value}" ItemsSource="{Binding cbb_C_ItemSource}" />

                                        <CheckBox  Grid.Row="4" Grid.Column="0" IsChecked="{Binding chb_PointD_SplitLineSetting}" />
                                        <Label Content="D值(打點位置)"  Grid.Row="4" Grid.Column="1" VerticalAlignment="Center" />
                                        <ComboBox Grid.Row="4" Grid.Column="2" SelectedValue="{Binding PointD_Value}" ItemsSource="{Binding cbb_D_ItemSource}" />

                                        <CheckBox  Grid.Row="5" Grid.Column="0" VerticalAlignment="Center" IsChecked="{Binding chb_Thickness_SplitLineSetting}" />
                                        <Label Content="切割厚度："  Grid.Row="5" Grid.Column="1" VerticalAlignment="Center" />
                                        <TextBox Grid.Row="5" Grid.Column="2" VerticalAlignment="Center" >
                                            <TextBox.Text>
                                                <Binding Path="CutThickness" StringFormat="{}" UpdateSourceTrigger="PropertyChanged" >
                                                    <Binding.ValidationRules>
                                                        <STD105ValidRule:CheckNumberRule NumberMin="0"/>
                                                    </Binding.ValidationRules>
                                                </Binding>
                                            </TextBox.Text>
                                        </TextBox>
                                        <Label Grid.Row="5" Grid.Column="3" Content="(單位：mm)" VerticalAlignment="Center" Margin="3,0"/>

                                        <CheckBox  Grid.Row="6" Grid.Column="0" VerticalAlignment="Center" IsChecked="{Binding chb_RemainingLength_SplitLineSetting}" />
                                        <Label Content="餘料長度設定"  Grid.Row="6" Grid.Column="1" VerticalAlignment="Center" />
                                        <Grid Grid.Row="6" Grid.Column="2" >
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <Label Content ="大於"   Grid.Column="0"  VerticalAlignment="Center" Margin="0,0,5,0" />

                                            <TextBox  Grid.Column="1" VerticalAlignment="Center">
                                                <TextBox.Text>
                                                    <Binding Path="SplitRemainingLength" StringFormat="{}" UpdateSourceTrigger="PropertyChanged" >
                                                        <Binding.ValidationRules>
                                                            <STD105ValidRule:CheckNumberRule NumberMin="0"/>
                                                        </Binding.ValidationRules>
                                                    </Binding>
                                                </TextBox.Text>
                                            </TextBox>
                                        </Grid>
                                        <Label Grid.Row="6" Grid.Column="3" Content="(單位：mm)" VerticalAlignment="Center" Margin="3,0" />
                                        <Label Content=" (小於餘料：廢料"  Grid.Row="7" Grid.Column="1" VerticalAlignment="Center" />
                                    </Grid>
                                </GroupBox>

                                <StackPanel Grid.Row="3"
                            Orientation="Horizontal"
                                Margin="10"
                            HorizontalAlignment="Right">
                                    <Button 
                            Content="新增"
                            Command="{Binding NewSplitLineCommand}"
                            FontSize="{ Binding SubTitleFontSize, Source={ x:Static SofSetting:SofSetting.Default } }"
                            VerticalContentAlignment="Center"
                            Height="30"
                            Width="80"
                            Margin="10"/>
                                    <Button 
                            Command="{Binding ToggleAllSplitLineCheckboxCommand}"
                            FontSize="{ Binding SubTitleFontSize, Source={ x:Static SofSetting:SofSetting.Default } }"
                            VerticalContentAlignment="Center"
                             Style="{StaticResource SiegCustomButtonGray}"
                            Height="30"
                            Width="80"
                            Margin="10">
                                        <Button.Content>
                                            <StackPanel Orientation="Horizontal">
                                                <wpf:PackIcon Kind="CheckBoxOutline"/>
                                                <TextBlock Text="／" Margin="0,-2,0,0"/>
                                                <wpf:PackIcon Kind="CheckboxBlankOutline"/>
                                            </StackPanel>
                                        </Button.Content>
                                    </Button>
                                    <Button 
                            Content="復原"
                            Command="{Binding GoBackSplitLineCommand}"
                            FontSize="{ Binding SubTitleFontSize, Source={ x:Static SofSetting:SofSetting.Default } }"
                            VerticalContentAlignment="Center"
                            Height="30"
                            Width="80"
                            Margin="10"/>
                                </StackPanel>


                            </Grid>

                        </GroupBox>
                    </Border>
                </Grid>

            </TabItem>
            <TabItem Header="型鋼加工區域設定" >
                <Grid   dx:ThemeManager.ThemeName="Win10Light" >
                    <Grid.DataContext>
                        <STD105VM:SettingParVM/>
                    </Grid.DataContext>
                    <Grid.Resources>
                        <Style TargetType="GroupBox"
                               BasedOn="{ StaticResource ParameterBaseStyleGroupBox }">
                        </Style>

                        <SolidColorBrush x:Key="PrimaryHueMidBrush" Color="{StaticResource WordBlue}"/>
                        <Style TargetType="Button"  BasedOn="{StaticResource MaterialDesignFlatButton}"   >
                            <Setter Property="Background" Value="White"/>
                            <Setter Property="Foreground" Value="Black"/>
                            <Setter Property="BorderBrush" Value="#CFCFCF"/>
                            <Setter Property="BorderThickness" Value="1"/>
                            <Setter Property="FontSize" Value="14"/>
                        </Style>

                        <Style TargetType="Label">
                            <Setter Property="FontSize" Value="14"/>
                        </Style>


                        <Style TargetType="TextBox" BasedOn="{StaticResource MaterialDesignTextBoxBase}">
                            <Setter Property="Foreground" Value="{ StaticResource solidclr_DimGray }" />
                            <Setter Property="FontSize" Value="14" />
                            <Setter Property="Padding" Value="1" />
                            <Setter Property="FontFamily" Value="Microsoft JhengHei"/>
                            <Setter Property="Background" Value="White"/>
                            <Setter Property="BorderThickness" Value="1"/>
                            <Setter Property="BorderBrush" Value="Gray"/>
                        </Style>

                        <Style TargetType="CheckBox" BasedOn="{StaticResource BaseStyle}">
                            <Setter Property="VerticalAlignment" Value="Center"/>
                        </Style>
                        <Style TargetType="ComboBox"
                   BasedOn="{ StaticResource BaseStyle }">
                            <Setter Property="Height" Value="24"/>
                            <Setter Property="FontSize" Value="16"/>
                        </Style>
                    </Grid.Resources>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="auto" />
                    </Grid.ColumnDefinitions>
                    <!--型鋼加工區域設定-->
                    <Border BorderThickness="1" BorderBrush="Gray" Width="700" >
                        <GroupBox Header="型鋼加工區域設定" Style="{StaticResource ParameterGroupBox}">
                            <Grid>
                                <Grid.Resources>
                                    <std:OBJECT_TYPE_ToStringConverter x:Key="obType" />
                                </Grid.Resources>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition />
                                    <ColumnDefinition Width="5" />
                                </Grid.ColumnDefinitions>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="auto"/>
                                    </Grid.RowDefinitions>

                                    <GroupBox Grid.Row="0" Header="種類設定" Margin="5" Background="White" Padding="20">
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="auto"/>
                                                <ColumnDefinition Width="150"/>
                                                <ColumnDefinition Width="auto"/>
                                                <ColumnDefinition Width="150"/>
                                            </Grid.ColumnDefinitions>
                                            <Grid.Resources>
                                                <ST:ProcessingBehavior_ToStringConverter x:Key="keyProcessingBehavior" />
                                            </Grid.Resources>

                                            <CheckBox Grid.Column="0" IsChecked="{Binding chb_SteelType}" Content="型鋼型態"/>
                                            <ComboBox Width="100" 
                                               Grid.Column="1"
                                          HorizontalContentAlignment="Center"
                                          VerticalContentAlignment="Center"
                                          x:Name="cbx_SteelType"
                                          SelectedIndex="{Binding SelectSectionType}">
                                                <i:Interaction.Triggers>
                                                    <i:EventTrigger EventName="SelectionChanged" SourceName="cbx_SteelType">
                                                        <i:InvokeCommandAction Command="{ Binding ShowProcessingZoneCommand }"
                                                                   CommandParameter="{ Binding ElementName=cbx_SteelType, Path=SelectedIndex }" />
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>
                                                <ComboBoxItem>
                                                    <Image Source="{ StaticResource H_TYPE }" Height="15" Width="15"/>
                                                </ComboBoxItem>
                                                <ComboBoxItem>
                                                    <Image Source="{ StaticResource BOX_TYPE }" Height="15" Width="15"/>
                                                </ComboBoxItem>
                                                <ComboBoxItem>
                                                    <Image Source="{ StaticResource CH_TYPE }" Height="15" Width="15"/>
                                                </ComboBoxItem>
                                            </ComboBox>
                                            <CheckBox  Grid.Column="2" IsChecked="{Binding chb_ProcessingBehavior}" Content="加工方式" />
                                            <ComboBox Width="100"   Grid.Column="3"
                                          Margin="5,0" 
                                          VerticalContentAlignment="Center"
                                          HorizontalContentAlignment="Center"
                                          SelectedIndex="{Binding SelectProcessingBehavior}"
                                          ItemsSource="{Binding Source={StaticResource keyProcessingBehavior} , Path=Strings}" 
                                          x:Name="cbx_ProcessingBehavior">
                                                <i:Interaction.Triggers>
                                                    <i:EventTrigger EventName="SelectionChanged" SourceName="cbx_ProcessingBehavior">
                                                        <i:InvokeCommandAction Command="{ Binding ShowProcessingSettingValueCommand }"
                                                                   CommandParameter="{ Binding ElementName=cbx_ProcessingBehavior, Path=SelectedIndex }" />
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>
                                            </ComboBox>

                                        </Grid>
                                    </GroupBox>

                                    <!--加工區域設定-->
                                    <GroupBox Grid.Row="1" Header="加工區域設定"  Margin="5" Background="White" MinHeight="340" Padding="20">
                                        <GroupBox.Resources>
                                            <Style TargetType="Line">
                                                <Setter Property="Stroke" Value="Black"/>
                                                <Setter Property="StrokeThickness" Value="1"/>
                                            </Style>
                                        </GroupBox.Resources>

                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="auto"/>
                                                <RowDefinition Height="auto"/>
                                                <RowDefinition Height="auto"/>
                                            </Grid.RowDefinitions>
                                            <!--H型鋼-->
                                            <Grid Visibility="{Binding VisibleH_Process, Converter={WPFBase:NoNullBooleanToVisibilityConverter}}">

                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*"/>
                                                    <ColumnDefinition Width="auto"/>
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="*"/>
                                                </Grid.RowDefinitions>
                                                <Grid  Grid.Row="0" Grid.Column="0">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="40" />
                                                        <RowDefinition Height="40" />
                                                        <RowDefinition Height="40" />
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="auto"/>
                                                        <ColumnDefinition Width="auto"/>
                                                        <ColumnDefinition Width="auto"/>
                                                        <ColumnDefinition Width="*" MinWidth="100"/>
                                                        <ColumnDefinition/>
                                                    </Grid.ColumnDefinitions>

                                                    <CheckBox IsChecked="{Binding chb_H_Avalue}" Grid.Row="0" Grid.Column="0" VerticalAlignment="Center"/>
                                                    <Label Content="A值："  Grid.Row="0" Grid.Column="1" VerticalAlignment="Center"/>
                                                    <Label Content="翼板厚度+"  Grid.Row="0" Grid.Column="2" VerticalAlignment="Center"/>
                                                    <TextBox  Grid.Row="0" Grid.Column="3" VerticalAlignment="Center" Margin="5,0">
                                                        <TextBox.Text>
                                                            <Binding Path="ProcessingZone_A" StringFormat="{}" UpdateSourceTrigger="PropertyChanged" >
                                                                <Binding.ValidationRules>
                                                                    <STD105ValidRule:CheckNumberRule NumberMin="40"/>
                                                                </Binding.ValidationRules>
                                                            </Binding>
                                                        </TextBox.Text>
                                                    </TextBox>
                                                    <Label Grid.Row="0" Grid.Column="4" VerticalAlignment="Center" Content="(不得小於40mm)" Foreground="DarkRed" Margin="5,0"/>

                                                    <CheckBox IsChecked="{Binding chb_H_Bvalue}" Grid.Row="1" Grid.Column="0" VerticalAlignment="Center" />
                                                    <Label Content="B值："  Grid.Row="1" Grid.Column="1" VerticalAlignment="Center" />
                                                    <TextBox  Grid.Row="1" Grid.Column="3" VerticalAlignment="Center" Margin="5,0">
                                                        <TextBox.Text>
                                                            <Binding Path="ProcessingZone_B" StringFormat="{}" UpdateSourceTrigger="PropertyChanged" Mode="TwoWay">
                                                                <Binding.ValidationRules>
                                                                    <STD105ValidRule:CheckNumberRule NumberMin="10"/>
                                                                </Binding.ValidationRules>
                                                            </Binding>
                                                        </TextBox.Text>
                                                    </TextBox>
                                                    <Label Grid.Row="1" Grid.Column="4" VerticalAlignment="Center" Content="(不得小於10mm)" Foreground="DarkRed" Margin="5,0"/>

                                                    <CheckBox IsChecked="{Binding chb_H_Cvalue}" Grid.Row="2" Grid.Column="0" VerticalAlignment="Center" />
                                                    <Label Content="C值："  Grid.Row="2" Grid.Column="1" VerticalAlignment="Center" />
                                                    <Label Content="翼板厚度+"  Grid.Row="2" Grid.Column="2" VerticalAlignment="Center" />
                                                    <TextBox  Grid.Row="2" Grid.Column="3" VerticalAlignment="Center" Margin="5,0">
                                                        <TextBox.Text>
                                                            <Binding Path="ProcessingZone_C" StringFormat="{}" UpdateSourceTrigger="PropertyChanged" >
                                                                <Binding.ValidationRules>
                                                                    <STD105ValidRule:CheckNumberRule NumberMin="3"/>
                                                                </Binding.ValidationRules>
                                                            </Binding>
                                                        </TextBox.Text>
                                                    </TextBox>
                                                    <Label Grid.Row="2" Grid.Column="4" VerticalAlignment="Center" Content="(不得小於3mm)" Foreground="DarkRed" Margin="5,0" />

                                                </Grid>
                                                <Grid Grid.Column="1" Margin="10,0">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="auto"/>
                                                        <ColumnDefinition Width="auto"/>
                                                    </Grid.ColumnDefinitions>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition/>
                                                        <RowDefinition/>
                                                        <RowDefinition Height="auto"/>
                                                    </Grid.RowDefinitions>
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="52"/>
                                                            <ColumnDefinition Width="91"/>
                                                            <ColumnDefinition Width="46"/>
                                                            <ColumnDefinition Width="*"/>
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Text="A" Grid.Column="0" HorizontalAlignment="Center" FontWeight="Bold"/>
                                                        <TextBlock Text="可加工區域" Grid.Column="1" HorizontalAlignment="Center" />
                                                        <TextBlock Text="A" Grid.Column="2" HorizontalAlignment="Center" FontWeight="Bold"/>
                                                    </Grid>
                                                    <Image Grid.Row="1"  Source="{ StaticResource H_PROCESS_ZONE }"  Height="auto" Width="220"/>
                                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center"  Grid.Row="2" Margin="0,10,25,0">
                                                        <Label Content="■" VerticalAlignment="Bottom" Foreground="DodgerBlue"/>
                                                        <Label Content="可加工區域" />
                                                    </StackPanel>
                                                    <Grid Grid.Column="1" Grid.Row="1">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition></ColumnDefinition>
                                                        </Grid.ColumnDefinitions>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="39"/>
                                                            <RowDefinition Height="19"/>
                                                            <RowDefinition Height="58"/>
                                                            <RowDefinition Height="38"/>
                                                            <RowDefinition Height="58"/>
                                                            <RowDefinition/>
                                                        </Grid.RowDefinitions>

                                                        <Grid Grid.Column="0" Grid.Row="1">
                                                            <Line X1="0" X2="10" Y1="10" Y2="10"/>
                                                            <Line X1="10" X2="20" Y1="10" Y2="0"/>
                                                            <Line X1="20" X2="30" Y1="0" Y2="0"/>
                                                        </Grid>

                                                        <Grid Grid.Column="0" Grid.Row="3">
                                                            <Line X1="0" X2="10" Y1="20" Y2="20"/>
                                                            <Line X1="10" X2="20" Y1="20" Y2="0"/>
                                                            <Line X1="20" X2="30" Y1="0" Y2="0"/>
                                                        </Grid>
                                                        <Grid Grid.Column="0" Grid.Row="5">
                                                            <Line X1="0" X2="10" Y1="10" Y2="10"/>
                                                            <Line X1="10" X2="20" Y1="10" Y2="0"/>
                                                            <Line X1="20" X2="30" Y1="0" Y2="0"/>
                                                        </Grid>
                                                        <TextBlock Text="B" Grid.Row="0" HorizontalAlignment="Right" VerticalAlignment="Bottom" FontWeight="Bold" Height="16"  Margin="0,2"/>
                                                        <TextBlock Text="C" Grid.Row="2" HorizontalAlignment="Right" VerticalAlignment="Bottom" FontWeight="Bold" Height="16"  Margin="0,2"/>
                                                        <TextBlock Text="B" Grid.Row="4" HorizontalAlignment="Right" VerticalAlignment="Bottom" FontWeight="Bold" Height="16"  Margin="0,2"/>
                                                    </Grid>

                                                </Grid>

                                            </Grid>
                                            <!--方管-->
                                            <Grid Grid.Row="1" Visibility="{Binding VisibleBOX_Process, Converter={WPFBase:NoNullBooleanToVisibilityConverter}}" >

                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*"/>
                                                    <ColumnDefinition Width="auto"/>
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="*"/>
                                                </Grid.RowDefinitions>

                                                <Grid  Grid.Row="0" Grid.Column="0">
